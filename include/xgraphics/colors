#ifndef XQLIB_WEB_COLORS_HPP
#define XQLIB_WEB_COLORS_HPP

#include <glm/glm.hpp>

//! Provides vec4 definitions of all colors defined in
//! the [XKCD colors](https://xkcd.com/color/rgb/).
namespace xgraphics::colors
{
	namespace _detail
	{
	    constexpr int hex2num(char c)
	    {
	        if(c >= 'A' and c <= 'F')
	            return c - 'A' + 0xA;
	        else if(c >= 'a' and c <= 'f')
	            return c - 'a' + 0xa;
	        else if(c >= '0' and c <= '9')
	            return c - '0';
	        else
	            return 0;
	    }

	    constexpr int hex2num(char c1, char c0)
	    {
	        return (hex2num(c1) << 4) | hex2num(c0);
	    }

	    constexpr float hex2flt(char c) { return hex2num(c) / 15.0f; }
	    constexpr float hex2flt(char c1, char c0) { return hex2num(c1, c0) / 255.0f; }
	}

	// user defined HTML color literal
	constexpr glm::vec4 operator "" _col (char const * str, std::size_t len)
	{
	    if(len == 9) // "#RRGGBBAA"
	        return glm::vec4 { _detail::hex2flt(str[1], str[2]),  _detail::hex2flt(str[3], str[4]),  _detail::hex2flt(str[5], str[6]), _detail::hex2flt(str[7], str[8]) };
	    else if(len == 8) // "RRGGBBAA"
	        return glm::vec4 { _detail::hex2flt(str[0], str[1]),  _detail::hex2flt(str[2], str[3]),  _detail::hex2flt(str[4], str[5]), _detail::hex2flt(str[6], str[7]) };
	    if(len == 7) // "#RRGGBB"
	        return glm::vec4 { _detail::hex2flt(str[1], str[2]),  _detail::hex2flt(str[3], str[4]),  _detail::hex2flt(str[5], str[6]), 1.0 };
	    else if(len == 6) // "RRGGBB"
	        return glm::vec4 { _detail::hex2flt(str[0], str[1]),  _detail::hex2flt(str[2], str[3]),  _detail::hex2flt(str[4], str[5]), 1.0 };
	    else if(len == 4) // "#RGB"
	        return glm::vec4 { _detail::hex2flt(str[1]),  _detail::hex2flt(str[2]),  _detail::hex2flt(str[3]), 1.0 };
	    else if(len == 3) // "RGB"
	        return glm::vec4 { _detail::hex2flt(str[0]),  _detail::hex2flt(str[1]),  _detail::hex2flt(str[2]), 1.0 };
	    else
	        return glm::vec4 { 1,0,1,1 };
	}

